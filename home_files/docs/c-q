"union tag1 {
    unsigned int a;
    unsigned int d;
    unsigned char b;
    unsigned char c;
} un;

int main()
{
un.a = 498;
un.d = un.d & 0xceba;
printf(""un.a  = %d un.b = %d un.c %d un.d = %d\n"",un.a,un.b, un.c, un.d);
}"

