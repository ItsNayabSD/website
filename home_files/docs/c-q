int main(void) {
   int i, arr[5] = {20, 30, 40, 50, 60};
   for (i = 0; i < 5;i++) {
       printf(""%d "",*arr);
       arr++;
   }

} 


int main(void) {
   int i, size, arr[size];

   for (i = 0; i < size;i++) {
       scanf(""%d"",&arr[i]);
   }
   for (i = 0; i < size;i++) {
       printf(""%d "",arr[i]);
   }

} 

"#include<stdio.h>

void function(int *p) 
{
    int c=10;
    p=&c;
}

int main()
{
    int a=20;
    int *p=&a;
    function(p);
    printf(""%d\n"",*p);
    return 0;
}

